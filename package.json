        "@swc-node/register": "~1.6.7",
        "@swc/core": "~1.3.85",
        "@swc/helpers": "~0.5.2",
        "@testing-library/react": "14.0.0",
        "@types/jest": "^29.4.0",
        "@types/node": "18.16.9",
        "@types/passport-jwt": "^4.0.1",
        "@types/react": "18.2.33",
        "@types/react-dom": "18.2.14",
        "@typescript-eslint/eslint-plugin": "^6.13.2",
