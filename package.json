{
    "name": "@aofg/source",
    "version": "0.0.1",
    "license": "MIT",
    "scripts": {
        "dev": "cross-env FORCE_COLOR=true && concurrently -n \"FRONT,BACKE,TELEG,DISCO\" -c \"bgBlue.bold,bgGreen.bold,bgRed.bold,bgYellow.bold\" \"npm:serve:frontend\" \"npm:serve:backend\" \"npm:serve:telegram\" \"npm:serve:discord\"",
        "serve": "",
        "serve:frontend": "nx dev frontend",
        "serve:backend": "nx serve backend",
        "serve:telegram": "nx serve telegram-bot",
        "serve:discord": "nx serve discord-bot",
        "build": "nx run-many --target=build:production --projects=frontend,backend,telegram-bot,discord-bot  --prod",
        "build:backend": "nx build backend --prod",
        "build:frontend": "nx build frontend --prod",
        "build:telegram": "nx build telegram-bot --prod",
        "build:discord": "nx build discord-bot --prod",
        "start": "npm i && npm run dev",
        "typeorm-build-config": "tsc ./lib/database/src/typeorm.config.ts --outDir ./dist",
        "typeorm-generate-migrations": "npm run typeorm-build-config && typeorm -d dist/typeorm.config.js migration:generate",
        "typeorm-run-migrations": "npm run typeorm-build-config && typeorm -d dist/typeorm.config.js migration:run",
        "prepare": "husky install"
    },
    "private": true,
    "dependencies": {
        "@nestjs/common": "^10.0.2",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.0.2",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/microservices": "^10.3.1",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.0.2",
        "@nestjs/swagger": "^7.3.0",
        "@nestjs/typeorm": "^10.0.2",
        "@nx/nest": "^18.0.0",
        "@sinclair/typebox": "^0.32.13",
        "@telegraf/types": "^7.1.0",
        "amqp-connection-manager": "^4.1.14",
        "amqplib": "^0.10.3",
        "axios": "^1.6.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.2",
        "nestjs-telegraf": "^2.7.0",
        "next": "14.0.4",
        "passport-jwt": "^4.0.1",
        "pg": "^8.11.3",
        "postgres": "^3.4.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.0",
        "telegraf": "^4.16.0",
        "tslib": "^2.3.0",
        "typeorm": "^0.3.20"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.6.0",
        "@commitlint/config-conventional": "^18.6.0",
        "@nestjs/schematics": "^10.0.1",
        "@nestjs/testing": "^10.0.2",
        "@nx-tools/container-metadata": "^5.2.0",
        "@nx-tools/nx-container": "^5.2.0",
        "@nx/cypress": "18.0.0",
        "@nx/eslint": "18.0.0",
        "@nx/eslint-plugin": "18.0.0",
        "@nx/jest": "18.0.0",
        "@nx/js": "18.0.0",
        "@nx/next": "^18.0.0",
